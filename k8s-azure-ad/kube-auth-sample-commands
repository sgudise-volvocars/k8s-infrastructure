kubectl config set-credentials sagudise --auth-provider=azure \
  --auth-provider-arg=environment=AzurePublicCloud \
  --auth-provider-arg=client-id= \
  --auth-provider-arg=tenant-id= \
  --auth-provider-arg=apiserver-id=

